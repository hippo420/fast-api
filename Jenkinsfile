pipeline {
  agent any
  stages {
    stage('check out') {
      steps {
        sh 'echo "check out"'
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "deploy"'
      }
    }

    stage('Process After Deploy') {
      steps {
        sh 'echo "Process After Deploy"'
      }
    }

  }
}